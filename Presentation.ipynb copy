    "yymm_edges = []\n",
    "\n",
    "years = [2017, 2018, 2019, 2020]\n",
    "\n",
    "for y in years:\n",
    "    for m in np.arange(1, 12+1, 1):\n",
    "        yymm_edges.append(dt.date(y, m, 1))\n",
    "\n",
    "yymm_edges = yymm_edges[6:-4]\n",
    "\n",
    "yymm_ticks = []\n",
    "\n",
    "for i in np.arange(0,round(len(yymm_edges)/ 6)+1,1):\n",
    "    yymm_ticks.append(yymm_edges[(i*6)])"
   ]
  },
