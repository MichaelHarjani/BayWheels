    "\n",
    "plt.xlabel('')\n",
    "\n",
    "plt.yticks(np.arange(-1, 1+0.25, 0.25), ['-100%', '-75%', '-50%', '-25%', '0%', '25%', '50%', '75%', '100%'])\n",
    "plt.ylim([-1, 1])\n",
    "plt.ylabel('')\n",
    "\n",
    "plt.axhline(0, color='black')\n",
    "\n",
