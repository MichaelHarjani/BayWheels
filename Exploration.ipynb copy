    "# Creating a new column combines both 'user_type' and 'bsfa' as 'bsfa' is a subset within 'member' in 'user_type'\n",
    "\n",
    "oldcasual = oldcols[oldcols['user_type'] == 'Casual']\n",
    "newcasual = newcols[newcols['user_type'] == 'Casual']\n",
